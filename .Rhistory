filter(month == 'March')
uber_apr <- uber %>%
filter(month == 'April')
uber_may <- uber %>%
filter(month == 'May')
uber_jun <- uber %>%
filter(month == 'June')
# Create Saturday tables to make comparisons
saturday_rides <- uber%>%
filter(day == 'Saturday') %>%
group_by(month, date, day, hour) %>%
summarise(rides = n()) %>%
ungroup()
